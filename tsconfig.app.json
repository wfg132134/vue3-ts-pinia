{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue", "auto-imports.d.ts", "components.d.ts"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "noImplicitAny": false,
    "composite": true,
    "noEmit": true,
    "baseUrl": ".",
    "strict": false,
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["vite/client"]
  }
}
